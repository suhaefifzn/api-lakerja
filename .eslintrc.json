{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "eol-last": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "import/order": "off",
    "class-methods-use-this": "off",
    "consistent-return": "off"
  }
}
